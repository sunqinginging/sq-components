{
  "name": "sunqing-components",
  "version": "1.0.5",
  "description": "Vue3 component library",
  "keywords": [
    "Component",
    "Vue3"
  ],
  "repository": {
    "type": "git",
    "url": "https://sunqingest.github.io/sq-components-doc/"
  },
  "homepage": "https://sunqingest.github.io/sq-components/",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/sq-components.umd.cjs",
  "module": "./dist/sq-components.js",
